<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>io.suite.venus</groupId>
	<artifactId>venus-job</artifactId>
	<version>0.0.1</version>
	<packaging>pom</packaging>

	<name>${project.artifactId}</name>

	<modules>
		<module>venus-job-core</module>
		<module>venus-job-admin</module>
		<module>venus-job-demo</module>
    </modules>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<maven.compiler.encoding>UTF-8</maven.compiler.encoding>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
		<maven.test.skip>true</maven.test.skip>

		<xxl-rpc.version>1.4.0</xxl-rpc.version>

		<spring-boot.version>2.3.5.RELEASE</spring-boot.version>
		<mybatis-spring-boot-starter.version>2.2.0</mybatis-spring-boot-starter.version>
		<mysql-connector-java.version>5.1.47</mysql-connector-java.version>

		<spring.version>5.3.14</spring.version>
		<freemarker.version>2.3.28</freemarker.version>

		<slf4j-api.version>1.7.26</slf4j-api.version>
		<junit.version>4.12</junit.version>

		<commons-exec.version>1.3</commons-exec.version>

		<groovy.version>2.5.6</groovy.version>
		<quartz.version>2.3.2</quartz.version>
		<springfox.version>2.10.5</springfox.version>

		<maven-source-plugin.version>3.0.1</maven-source-plugin.version>
		<maven-javadoc-plugin.version>3.1.0</maven-javadoc-plugin.version>
		<maven-gpg-plugin.version>1.6</maven-gpg-plugin.version>
		<maven-war-plugin.version>3.2.2</maven-war-plugin.version>
	</properties>


</project>